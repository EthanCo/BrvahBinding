    <style name="AppThemeBase" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorLink">#0b9a27</item>
        <item name="android:textColorPrimary">@color/colorPrimaryText</item>
        <item name="android:windowBackground">@color/colorWhiteSmoke</item>
        <item name="actionMenuTextColor">@color/colorWhite</item>
        <item name="editTextColor">@color/colorSecondaryText</item>
        <!--<item name="actionBarSize">@dimen/actionbar_height</item>-->
        <item name="android:dialogTheme">@style/TextSizeDialog</item>
        <item name="android:textAppearanceLargePopupMenu">@style/myPopupMenuTextAppearanceLarge</item>

    </style>
    <style name="AppThemeDefault" parent="AppThemeBase"/>
    <style name="CustomSeekBar" parent="Base.Widget.AppCompat.SeekBar">
        <item name="android:progressDrawable">@drawable/layer_seekbar_track</item>
        <item name="android:indeterminateDrawable">@drawable/layer_seekbar_track</item>
        <item name="android:thumb">@drawable/selector_seekbar_thumb</item>
        <item name="android:minHeight">2dp</item>
        <item name="android:maxHeight">2dp</item>
    </style>
    <style name="FullAppTheme" parent="FullAppThemeBase">
    </style>
    <style name="FullAppThemeBase" parent="AppThemeBase">
        <item name="android:windowFullscreen">true</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>
    <style name="RecyclerViewOverScroll">
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="RippleBorder">
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="RippleBorderless">
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="RippleBorderlessItem">
        <item name="android:background">@drawable/ripple_item</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="RippleBorderlessItemSmall">
        <item name="android:background">@drawable/ripple_item</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="TextShadowTheme">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#88096dae</item>
        <item name="android:shadowDx">3</item>
        <item name="android:shadowDy">3</item>
        <item name="android:shadowRadius">8</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:typeface">monospace</item>
    </style>
    <style name="TextSizeDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:theme">@style/AppThemeDefault</item>
        <item name="android:textSize">16sp</item>
        <!--<item name="android:windowFullscreen">true</item>-->
    </style>
    <style name="Theme.AppStartLoadTranslucent" parent="FullAppTheme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="TransDialog" parent="Base.Theme.AppCompat.Dialog">
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->

        <item name="android:progressDrawable">@drawable/layer_seekbar_track</item>
        <item name="android:indeterminateDrawable">@drawable/layer_seekbar_track</item>
        <item name="android:thumb">@drawable/selector_seekbar_thumb</item>
    </style>
    <style name="myPopupMenuTextAppearanceLarge" parent="@style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Large">
        <item name="android:textSize">18sp</item>
    </style>
