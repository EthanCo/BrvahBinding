<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fengh.recyclerviewtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecyclerViewBindingActivity"
            android:label="@string/title_activity_recycler_view_binding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BRVAHBindingActivity"
            android:label="@string/title_activity_brvahbinding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PullToRefreshActivity"></activity>
        <activity android:name=".brvah_binding.view.BRVAHBinding2Activity" />
    </application>

</manifest>